package com.visiontech.services;

import com.visiontech.entities.Contacto_emergencia;
import java.util.List;

public interface ContactoEmergenciaService {
    List<Contacto_emergencia> list();
    void insert(Contacto_emergencia contactoEmergencia);
    Contacto_emergencia listId(int idContacto);
    void update(Contacto_emergencia contactoEmergencia);
    void delete(int idContacto);
    List<Contacto_emergencia> buscarPorNombre(String nombre);
}
